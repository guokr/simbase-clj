{
    "VSCH":{
        "summary": "Get the schema of a specified doctype", "complexity": "O(1)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    },
    "VREV":{
        "summary": "Revise the schema for a specified doctype", "complexity": "O(1)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            },
            {
                "name": "schema",
                "type": "string",
                "multiple": true
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    },
    "VADD":{
        "summary": "Add a document specified by the doctype, docid and probabilities", "complexity": "O(n)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            },
            {
                "name": "docid",
                "type": "integer"
            },
            {
                "name": "probs",
                "type": "double",
                "multiple": true
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    },
    "VAPND":{
        "summary": "Append a document specified by the doctype, docid and name-value pairs", "complexity": "O(n)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            },
            {
                "name": "docid",
                "type": "integer"
            },
            {
                "name": "pairs",
                "type": "string",
                "multiple": true
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    },
    "VRETR":{
        "summary": "Retrieve some similar documents of a document specified by the doctype and docid", "complexity": "O(1)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            },
            {
                "name": "docid",
                "type": "integer"
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    },
    "VGET":{
        "summary": "Get the probabilities of a document specified by the doctype and docid", "complexity": "O(1)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            },
            {
                "name": "docid",
                "type": "integer"
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    },
    "VPUT":{
        "summary": "Update a document specified by the doctype, docid and probabilities", "complexity": "O(n)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            },
            {
                "name": "docid",
                "type": "integer"
            },
            {
                "name": "probs",
                "type": "double",
                "multiple": true
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    },
    "VUPDT":{
        "summary": "Update a document specified by the doctype, docid and name-value pairs", "complexity": "O(n)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            },
            {
                "name": "docid",
                "type": "integer"
            },
            {
                "name": "pairs",
                "type": "string",
                "multiple": true
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    },
    "VDEL":{
        "summary": "Delete a document specified by the doctype and docid", "complexity": "O(1)",
        "arguments": [
            {
                "name": "doctype",
                "type": "key"
            },
            {
                "name": "docid",
                "type": "integer"
            }
        ],
        "since": "0.0.0",
        "group": "simbase"
    }
}

